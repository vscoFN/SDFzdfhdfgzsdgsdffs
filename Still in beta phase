local Player = Window:NewTab("Player")
local BlackMarketExchange = Player:NewSection("Speed Modifier")

BlackMarketExchange:NewButton("Speed Modifier V1", "You can disable the speed Modifier V1 in the bottom left", function()
    
    sethiddenproperty(game.Players.LocalPlayer.Character.Humanoid, "WalkSpeed", 50)
end)

BlackMarketExchange:NewButton("Speed Modifier V2", "You can disable the speed Modifier V2 in the bottom left", function()
    
    sethiddenproperty(game.Players.LocalPlayer.Character.Humanoid, "WalkSpeed", 100)
end)

BlackMarketExchange:NewButton("Speed Modifier V3", "You can disable the speed Modifier V3 in the bottom left", function()
    
    sethiddenproperty(game.Players.LocalPlayer.Character.Humanoid, "WalkSpeed", 200)
end)


