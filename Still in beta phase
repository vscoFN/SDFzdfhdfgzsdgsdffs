local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("V5 HUB", "Ocean")
-- themes LightTheme, DarkTheme, Grape Theme, Blood Theme, Midnight, Sentinel, Synapse

-- MAIN
local BlackMarket = Window:NewTab("BlackMarket")
local BlackMarketExchange = BlackMarket:NewSection("Exchange Btc")

BlackMarketExchange:NewButton("Exchange Btc", "Exchange BTC from anywere", function()
    local ohBoolean1 = true

game:GetService("ReplicatedStorage").Events.ExchangeMoney:FireServer(ohBoolean1)
end)


